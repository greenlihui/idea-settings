<templateSet group="MyGroup">
  <template name="main" value="public static void main(String[] args) {&#10;    $END$&#10;}" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="ocom" value="/********** $END$ **********/" description="one line comment" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="tryjava" value="try {&#10;    $try$&#10;} catch (Exception e) {&#10;    e.printStackTrace();&#10;} finally {&#10;    $FINALLY$&#10;}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="try" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="FINALLY" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="clg" value="console.log($END$);" description="console.log" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="tryjs" value="try {&#10;    $TRY$&#10;} catch (err) {&#10;    $CATCH$&#10;}" description="try catch for js" toReformat="true" toShortenFQNames="true">
    <variable name="TRY" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CATCH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="dcom" value="/**&#10; * @param {$type$} $NAME$&#10; * @return {$TYPE$} $NAME$&#10; */" description="document comment" toReformat="true" toShortenFQNames="true">
    <variable name="type" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="mcom" value="/*&#10; * $END$&#10; */" description="multiple-lines comment" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="syso" value="System.out.println($END$);" description="System.out.printlb()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>